<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="io.github.sunny.cherry.tomato.account.mapper.TxMsgMapper">
    <resultMap id="BaseResultMap" type="io.github.sunny.cherry.tomato.account.model.TxMsg">
        <!-- Don't change -->
        <result column="id" jdbcType="VARCHAR" property="id"/>
        <result column="action" jdbcType="VARCHAR" property="action"/>
        <result column="modifier_time" jdbcType="TIMESTAMP" property="modifierTime"/>
    </resultMap>

    <select id="selectByPrimaryKey" resultMap="BaseResultMap">
        select id,`action`,modifier_time from tx_msg where id=#{id,jdbcType=VARCHAR}
    </select>
    <insert id="insertSelective" parameterType="io.github.sunny.cherry.tomato.account.model.TxMsg">
        <!-- Don't change -->
        insert into tx_msg
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="id != null">
                id,
            </if>
            <if test="action != null">
                action,
            </if>
            <if test="modifierTime != null">
                modifier_time,
            </if>
        </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="id != null">
                #{id,jdbcType=VARCHAR},
            </if>
            <if test="action != null">
                #{action,jdbcType=VARCHAR},
            </if>
            <if test="modifierTime != null">
                #{modifierTime,jdbcType=TIMESTAMP},
            </if>
        </trim>
    </insert>
</mapper>