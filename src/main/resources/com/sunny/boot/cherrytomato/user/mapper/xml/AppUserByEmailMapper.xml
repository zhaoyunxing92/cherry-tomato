<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.sunny.boot.cherrytomato.user.mapper.AppUserByEmailMapper">
    <resultMap id="BaseResultMap" type="com.sunny.boot.cherrytomato.user.model.AppUserByEmail">
        <!-- Don't change -->
        <id column="EMAIL" property="email" jdbcType="VARCHAR"/>
        <result column="USER_ID" property="userId" jdbcType="BIGINT"/>
    </resultMap>

    <!--根据邮箱获取用户id-->
    <select id="selectUserIdByPrimaryKey" resultType="java.lang.Long">
        select
        USER_ID
        from app_user_by_email
        where EMAIL = #{email,jdbcType=VARCHAR}
    </select>

    <delete id="deleteByPrimaryKey" parameterType="java.lang.String">
        <!-- Don't change -->
        delete from app_user_by_email
        where EMAIL = #{email,jdbcType=VARCHAR}
    </delete>

    <insert id="insert" parameterType="com.sunny.boot.cherrytomato.user.model.AppUserByEmail">
        <!-- Don't change -->
        insert into app_user_by_email (EMAIL, USER_ID)
        values (#{email,jdbcType=VARCHAR}, #{userId,jdbcType=BIGINT})
    </insert>

    <insert id="insertSelective" parameterType="com.sunny.boot.cherrytomato.user.model.AppUserByEmail">
        <!-- Don't change -->
        insert into app_user_by_email
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="email != null">
                email,
            </if>
            <if test="userId != null">
                user_id,
            </if>
        </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="email != null">
                #{email,jdbcType=VARCHAR},
            </if>
            <if test="userId != null">
                #{userId,jdbcType=BIGINT},
            </if>
        </trim>
    </insert>

</mapper>